{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/index.js",
            "methods": ["Get","Post","Put","Patch","Delete","Options"]
        }
    ]
}